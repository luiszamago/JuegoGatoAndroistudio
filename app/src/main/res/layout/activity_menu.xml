<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AAD5DA"
    android:scrollbarStyle="insideOverlay"
    tools:context=".MenuActivity">

    <Button
        android:id="@+id/button8"
        style="@android:style/Widget.Material.Button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="39dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="29dp"
        android:fontFamily="@font/fredoka_one"
        android:onClick="pasatiempos"
        android:text="Pasa Tiempos"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#F00E0D0D"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFichaTecnica"
        tools:visibility="visible" />

    <Button
        android:id="@+id/btnFichaTecnica"
        style="@android:style/Widget.Material.Button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="19dp"
        android:fontFamily="@font/fredoka_one"
        android:onClick="fichatecnica"
        android:text="Ficha Tecnica"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#F00E0D0D"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toEndOf="@+id/button8"
        app:layout_constraintStart_toStartOf="@+id/button8"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Ficha Tecnica" />

    <Button
        android:id="@+id/button9"
        style="@android:style/Widget.Material.Button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="22dp"
        android:fontFamily="@font/fredoka_one"
        android:onClick="KARDEX"
        android:text="Kardex"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#F00E0D0D"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button10"
        style="@android:style/Widget.Material.Button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="59dp"
        android:fontFamily="@font/fredoka_one"
        android:onClick="fotos"
        android:text="Fotos"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#F00E0D0D"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnAtras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/btnAtras"
        style="@android:style/Widget.DeviceDefault.Button.Small"
        android:layout_width="136dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/carter_one"
        android:onClick="onClick"
        android:text="Atras"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10" />

</android.support.constraint.ConstraintLayout>